              "type": "fill"
            }
          ],
          "measurement": "nodestatuscount_slurm",
          "orderByTime": "ASC",
          "policy": "default",
          "refId": "{{ loop.index }}",
          "resultFormat": "time_series",
          "select": [
            [
              {
                "params": [
                  "{{ entry.name }}"
                ],
                "type": "field"
              },
              {
                "params": [],
